<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="LibLite.Brushes.Gradient.Linear.Vertical.Primary" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="{StaticResource MahApps.Colors.Accent}" Offset="0"/>
        <GradientStop Color="{StaticResource MahApps.Colors.Accent2}" Offset="0.75"/>
        <GradientStop Color="{StaticResource MahApps.Colors.Accent4}" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="LibLite.Brushes.Gradient.Linear.Horizontal.Primary" StartPoint="0,1" EndPoint="1,1">
        <GradientStop Color="{StaticResource MahApps.Colors.Accent}" Offset="0"/>
        <GradientStop Color="{StaticResource MahApps.Colors.Accent2}" Offset="0.75"/>
        <GradientStop Color="{StaticResource MahApps.Colors.Accent4}" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="LibLite.Brushes.Purchase.Primary">#E69B00</SolidColorBrush>
    <SolidColorBrush x:Key="LibLite.Brushes.Product.Primary">#E6B400</SolidColorBrush>
    <SolidColorBrush x:Key="LibLite.Brushes.Group.Primary">#E6CC00</SolidColorBrush>
    <SolidColorBrush x:Key="LibLite.Brushes.Inventory.Primary">#E5DE00</SolidColorBrush>
</ResourceDictionary>